RewriteEngine On

# Redirigir la ra√≠z (/) a index.html
RewriteCond %{REQUEST_URI} ^/$
RewriteRule ^$ index.html

# Redirigir "projects" a /html/projects.html
RewriteCond %{REQUEST_URI} ^/projects$
RewriteRule ^projects$ /html/projects.html

# Redirigir "contact" a /html/contact.html
RewriteCond %{REQUEST_URI} ^/contact$
RewriteRule ^contact$ /html/contact.html

# Redigir "info" a /html/info.html
RewriteCond %{REQUEST_URI} ^/info$
RewriteRule ^info$ /html/info.html